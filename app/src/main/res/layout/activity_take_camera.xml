<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout_camera"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.humencheckworkattendance.ui.activity.TakeIdNumCameraActivity">


    <com.example.humencheckworkattendance.widget.CamearSurfaceChangeView
        android:id="@+id/cameraSurfaceView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageView
        android:id="@+id/imageView_TakePhote"
        android:layout_width="@dimen/x180"
        android:layout_height="@dimen/y180"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="@dimen/y70"
        app:srcCompat="@drawable/inserthumencameraphoto" />

    <ImageView
        android:id="@+id/imageView_Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/x24"
        android:layout_marginTop="@dimen/y76"
        app:srcCompat="@drawable/historyback" />

</FrameLayout>
